
.status_bar {
	height: var(--status-bar-height);  
	width: 100%;
	background-color: #bfd5ec;
}
.top_view {  
	height: var(--status-bar-height);  
	width: 100%;  
	position: fixed;  
	background-color: #bfd5ec;  
	top: 0;  
	z-index: 999;
}
@font-face {
	font-family: texticons;
	font-weight: normal;
	font-style: normal;
	src: url('https://at.alicdn.com/t/font_984210_5cs13ndgqsn.ttf') format('truetype');
}
.bgtop{
	width: 100%;
	height: 240rpx;
	background-color: #bfd5ec;
}  

.logo {
	width: 664rpx;
	padding: 34rpx 68rpx;
	margin: -139rpx auto 0 auto;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	background-color: #ffffff;
	border-radius: 10rpx;
	text-align: center;
	-webkit-box-shadow: 0 0 10px #ccc;
	        box-shadow: 0 0 10px #ccc;
}
.logo-img{
	width: 120rpx;
	height: 120rpx;
	border:4px solid #ffffff;
	margin-top:-98rpx;
	border-radius: 50%;
}
.uer-name{
	font-size: 34rpx;
	font-weight: blod;
}
.uni-section{
	overflow: hidden;
	margin-top: 30rpx;
}
.uni-section-item{
	width: 197rpx;
	height: 114rpx;
	background-color: #f6f6fe;
	border-radius: 10rpx;
	text-align: center;
}
.uni-section-item .img{
	width: 40rpx;
	height: 40rpx;
	margin-top:20rpx;
}
.uni-section-item .tit{
	color: #898989;
	font-size: 17rpx;
}
.center-list {
	margin-top: 20rpx;
	padding: 0 44rpx;
}
.center-list-item {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	padding: 30rpx 0;
}
.center-list-item .imgbox{
	width: 25%;
	text-align: center;
}
.center-list-item .imgbox .img{
	width: 30%;
	vertical-align: middle;
}
.center-list-item .titbox{
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
}
.center-list-item .titbox .tit{
	font-size: 26rpx;
	font-weight: bold;
}
.center-list-item .titbox .info{
	font-size: 22rpx;
	color: #666666;
}
.center-list-item .uni-icon{
	width: 30rpx;
	color: #dcdcdc;
}
.border-bottom {
	border-bottom-width: 1rpx;
	border-color: #dcdcdc;
	border-bottom-style: solid;
}
.btnbox{
	padding: 30rpx 0;
}
.btnbox .btn{
	width: 361rpx;
	height: 75rpx;
	margin:0 auto;
	line-height: 75rpx;
	color: #fff;
	font-size: 28rpx;
	text-align: center;
	background: url(data:image/png;base64,) round;
}



