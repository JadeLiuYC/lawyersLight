
.page-body{
	padding: 40rpx;
}
.inputbox .tit{
	font-size: 26rpx;
	color: #949494;
}
.inputbox .textarea{
	margin-top: 20rpx;
	width: 100%;
	height: 300rpx;
	background-color: #eeeeee;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	padding: 20rpx;
}
.btnbox{
	margin-top: 300rpx;
}
.btnbox button.btn{
	border-radius: 50rpx;
	color: #fff;
	font-size: 30rpx;
	background: #f47505;
}
.btnbox button.btn:after{
	border: none;
}
.button-hover[type=default]{
	background: #f47505;
}

